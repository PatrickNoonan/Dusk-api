{
    "name": "deltadefense/dusk",
    "description": "Dusk",
    "type": "project",
    "authors": [
        {
            "name": "Mike Georgeff",
            "email": "mike.georgeff@uscca.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.uscca.services"
        }
    ],
    "require": {
        "deltadefense/dusk-core": "1.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "1.7.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
